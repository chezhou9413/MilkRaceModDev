<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <HeadTypeDef ParentName="AverageBase">
        <defName>MunoRace_Head_A</defName>
        <graphicPath>AlienRace/MunoRace/Null/Null</graphicPath>
        <gender>None</gender>
    </HeadTypeDef>
    <AlienRace.ThingDef_AlienRace ParentName="BasePawn">
        <defName>MunoRace</defName>
        <label>奶牛娘</label>
        <description>一只奶牛娘</description>
        <alienRace>
            <raceRestriction>
                <onlyUseRaceRestrictedApparel>true</onlyUseRaceRestrictedApparel>
                <whiteXenotypeList MayRequire="Ludeon.RimWorld.Biotech">
                    <li>Baseliner</li>
                </whiteXenotypeList>
                <blackGeneTags MayRequire="Ludeon.RimWorld.Biotech">
                    <li>BeardStyle</li>
                    <li>HairStyle</li>
                    <li>Fur</li>
                    <li>SkinColorOverride</li>
                    <li>EyeColor</li>
                    <li>Tail</li>
                    <li>BodyType</li>
                    <li>Ears</li>
                    <li>Nose</li>
                    <li>Jaw</li>
                    <li>Headbone</li>
                </blackGeneTags>
                <blackEndoCategories>
                    <li>HairColor</li>
                    <li>Melanin</li>
                </blackEndoCategories>
                <onlyUseRaceRestrictedXenotypes>true</onlyUseRaceRestrictedXenotypes>
            </raceRestriction>
            <styleSettings>
                <li>
                    <key>HairDef</key>
                    <value>
                        <hasStyle>true</hasStyle>
                        <styleTagsOverride>
                            <li>Ag_Category</li>
                        </styleTagsOverride>
                    </value>
                </li>
                <li>
                    <key>BeardDef</key>
                    <value>
                        <hasStyle>false</hasStyle>
                    </value>
                </li>
                <li>
                    <key>TattooDef</key>
                    <value>
                        <hasStyle>false</hasStyle>
                    </value>
                </li>
            </styleSettings>
            <generalSettings>
                <maleGenderProbability>0</maleGenderProbability>
                <minAgeForAdulthood>18</minAgeForAdulthood>
                <immuneToXenophobia>false</immuneToXenophobia>
                <immuneToAge>false</immuneToAge>
                <humanRecipeImport>true</humanRecipeImport>
                <alienPartGenerator>
                    <customDrawSize>(1.3, 1.3)</customDrawSize>                    <!-- 整体大小设置 -->
                    <customHeadDrawSize>(0.8, 0.8)</customHeadDrawSize>                    <!-- 头部大小设置 -->
                    <customPortraitDrawSize>(1.3, 1.3)</customPortraitDrawSize>                    <!-- 头像身体位置设置 -->
                    <customPortraitHeadDrawSize>(0.8, 0.8)</customPortraitHeadDrawSize>                    <!-- 头像脑袋位置设置 -->
                    <headOffsetDirectional>
                        <north>
                            <offset>(0, 0.13)</offset>
                        </north>
                        <south>
                            <offset>(0, 0.13)</offset>
                        </south>
                        <east>
                            <offset>(-0.09, 0.12)</offset>
                        </east>
                        <west>
                            <offset>(0.09, 0.12)</offset>
                        </west>
                    </headOffsetDirectional>
                    <bodyTypes>
                        <li>Female</li>
                        <li MayRequire="Ludeon.RimWorld.Biotech">Baby</li>
                        <li MayRequire="Ludeon.RimWorld.Biotech">Child</li>
                    </bodyTypes>
                    <headTypes Inherit="False">
                        <li>MunoRace_Head_A</li>
                    </headTypes>
                    <colorChannels>
                        <li>
                            <name>skin</name>
                            <first Class="ColorGenerator_Single">
                                <color>RGB(255,255,255)</color>
                            </first>
                        </li>
                        <li>
                            <name>face</name>
                            <first Class="ColorGenerator_Single">
                                <color>RGB(255,255,255)</color>
                            </first>
                        </li>
                        <li>
                            <name>apparel</name>
                            <first Class="ColorGenerator_Single">
                                <color>RGB(255,255,255)</color>
                            </first>
                        </li>
                    </colorChannels>
                    <bodyAddons>
                        <li>
                            <name>MunoRace_head</name>
                            <userCustomizable>false</userCustomizable>
                            <path>AlienRace/MunoRace/head/head</path>
                            <colorChannel>skin</colorChannel>
                            <inFrontOfBody>true</inFrontOfBody>
                            <layerInvert>false</layerInvert>
                            <angle>0</angle>
                            <scaleWithPawnDrawsize>true</scaleWithPawnDrawsize>
                            <defaultOffset>Center</defaultOffset>
                            <alignWithHead>true</alignWithHead>
                            <drawSize>1.0</drawSize>
                            <conditions>
                                <BodyPart>
                                    <bodyPart>Head</bodyPart>
                                </BodyPart>
                                <Posture>
                                    <drawnStanding>true</drawnStanding>
                                    <drawnLaying>true</drawnLaying>
                                    <drawnInBed>true</drawnInBed>
                                </Posture>
                            </conditions>
                            <offsets>
                                <west>
                                    <layerOffset>0.1</layerOffset>
                                    <offset>(0,0)</offset>
                                </west>
                                <south>
                                    <layerOffset>0.1</layerOffset>
                                    <offset>(0,0)</offset>
                                </south>
                                <north>
                                    <layerOffset>0.1</layerOffset>
                                    <offset>(0,0)</offset>
                                </north>
                                <east>
                                    <layerOffset>0.1</layerOffset>
                                    <offset>(0,0)</offset>
                                </east>
                            </offsets>
                        </li>
                        <li>
                            <name>MunoRace_hair</name>
                            <userCustomizable>false</userCustomizable>
                            <path>AlienRace/MunoRace/hair/Hair</path>
                            <colorChannel>skin</colorChannel>
                            <inFrontOfBody>true</inFrontOfBody>
                            <layerInvert>false</layerInvert>
                            <angle>0</angle>
                            <scaleWithPawnDrawsize>true</scaleWithPawnDrawsize>
                            <defaultOffset>Center</defaultOffset>
                            <alignWithHead>true</alignWithHead>
                            <drawSize>1.0</drawSize>
                            <conditions>
                                <BodyPart>
                                    <bodyPart>Head</bodyPart>
                                </BodyPart>
                                <Posture>
                                    <drawnStanding>true</drawnStanding>
                                    <drawnLaying>true</drawnLaying>
                                    <drawnInBed>true</drawnInBed>
                                </Posture>
                            </conditions>
                            <offsets>
                                <west>
                                    <layerOffset>0.11</layerOffset>
                                    <offset>(0,0)</offset>
                                </west>
                                <south>
                                    <layerOffset>0.11</layerOffset>
                                    <offset>(0,0)</offset>
                                </south>
                                <north>
                                    <layerOffset>0.11</layerOffset>
                                    <offset>(0,0)</offset>
                                </north>
                                <east>
                                    <layerOffset>0.11</layerOffset>
                                    <offset>(0,0)</offset>
                                </east>
                            </offsets>
                        </li>
                        <li>
                            <name>MunoRace_hairBack</name>
                            <userCustomizable>false</userCustomizable>
                            <linkVariantIndexWithPrevious>true</linkVariantIndexWithPrevious>
                            <path>AlienRace/MunoRace/hairback/hairback</path>
                            <colorChannel>skin</colorChannel>
                            <inFrontOfBody>true</inFrontOfBody>
                            <layerInvert>false</layerInvert>
                            <drawSize>1</drawSize>
                            <angle>0</angle>
                            <scaleWithPawnDrawsize>true</scaleWithPawnDrawsize>
                            <defaultOffset>Center</defaultOffset>
                            <alignWithHead>true</alignWithHead>
                            <flipWest>true</flipWest>
                            <conditions>
                                <BodyPart>
                                    <bodyPart>Head</bodyPart>
                                </BodyPart>
                                <Posture>
                                    <drawnStanding>true</drawnStanding>
                                    <drawnLaying>true</drawnLaying>
                                    <drawnInBed>true</drawnInBed>
                                </Posture>
                            </conditions>
                            <offsets>
                                <west>
                                    <layerOffset>-0.31</layerOffset>
                                    <offset>(0,0)</offset>
                                </west>
                                <south>
                                    <layerOffset>-0.31</layerOffset>
                                    <offset>(0,0)</offset>
                                </south>
                                <north>
                                    <layerOffset>-0.31</layerOffset>
                                    <offset>(0,0)</offset>
                                </north>
                                <east>
                                    <layerOffset>-0.31</layerOffset>
                                    <offset>(0,0)</offset>
                                </east>
                            </offsets>
                        </li>
                        <li>
                            <name>MunoRace_horn</name>
                            <userCustomizable>false</userCustomizable>
                            <path>AlienRace/MunoRace/horn/Horn</path>
                            <colorChannel>skin</colorChannel>
                            <inFrontOfBody>true</inFrontOfBody>
                            <layerInvert>false</layerInvert>
                            <angle>0</angle>
                            <scaleWithPawnDrawsize>true</scaleWithPawnDrawsize>
                            <defaultOffset>Center</defaultOffset>
                            <alignWithHead>true</alignWithHead>
                            <drawSize>1.0</drawSize>
                            <conditions>
                                <BodyPart>
                                    <bodyPart>Head</bodyPart>
                                </BodyPart>
                                <Posture>
                                    <drawnStanding>true</drawnStanding>
                                    <drawnLaying>true</drawnLaying>
                                    <drawnInBed>true</drawnInBed>
                                </Posture>
                            </conditions>
                            <offsets>
                                <west>
                                    <layerOffset>0.15</layerOffset>
                                    <offset>(0,0)</offset>
                                </west>
                                <south>
                                    <layerOffset>0.15</layerOffset>
                                    <offset>(0,0)</offset>
                                </south>
                                <north>
                                    <layerOffset>0.15</layerOffset>
                                    <offset>(0,0)</offset>
                                </north>
                                <east>
                                    <layerOffset>0.15</layerOffset>
                                    <offset>(0,0)</offset>
                                </east>
                            </offsets>
                        </li>
                    </bodyAddons>
                </alienPartGenerator>
            </generalSettings>
            <graphicPaths>
                <skinShader>Cutout</skinShader>
                <head>
                    <path>AlienRace/MunoRace/Null/Null</path>
                </head>
                <body>
                    <path>AlienRace/MunoRace/body/Naked</path>
                </body>
            </graphicPaths>
            <thoughtSettings></thoughtSettings>
        </alienRace>
        <!--=====================================================================================================================-->
        <tools>
            <li>
                <label>left fist</label>
                <capacities>
                    <li>Blunt</li>
                </capacities>
                <power>8.2</power>
                <cooldownTime>2</cooldownTime>
                <linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
                <surpriseAttack>
                    <extraMeleeDamages>
                        <li>
                            <def>Stun</def>
                            <amount>14</amount>
                        </li>
                    </extraMeleeDamages>
                </surpriseAttack>
            </li>
            <li>
                <label>right fist</label>
                <capacities>
                    <li>Blunt</li>
                </capacities>
                <power>8.2</power>
                <cooldownTime>2</cooldownTime>
                <linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
                <surpriseAttack>
                    <extraMeleeDamages>
                        <li>
                            <def>Stun</def>
                            <amount>14</amount>
                        </li>
                    </extraMeleeDamages>
                </surpriseAttack>
            </li>
            <li>
                <label>teeth</label>
                <capacities>
                    <li>Bite</li>
                </capacities>
                <power>8.2</power>
                <cooldownTime>2</cooldownTime>
                <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
                <chanceFactor>0.07</chanceFactor>
                <soundMeleeHit>Pawn_Melee_HumanBite_Hit</soundMeleeHit>
                <soundMeleeMiss>Pawn_Melee_HumanBite_Miss</soundMeleeMiss>
            </li>
        </tools>
        <statBases>            <!-- 基础数据 -->
            <MarketValue>2750</MarketValue>
            <Flammability>1</Flammability>
            <MoveSpeed>4.8</MoveSpeed>
            <ComfyTemperatureMax>35</ComfyTemperatureMax>
            <ComfyTemperatureMin>10</ComfyTemperatureMin>
            <ImmunityGainSpeed>1.0</ImmunityGainSpeed>
            <CarryingCapacity>75</CarryingCapacity>
            <MeatAmount>80</MeatAmount>
            <LeatherAmount>50</LeatherAmount>
            <Mass>65</Mass>
            <PsychicSensitivity>0.8</PsychicSensitivity>
            <MeleeDodgeChance>1.2</MeleeDodgeChance>
            <SocialImpact>0.55</SocialImpact>
            <GeneralLaborSpeed>0.8</GeneralLaborSpeed>
            <DrugSynthesisSpeed>1.2</DrugSynthesisSpeed>
            <MedicalTendSpeed>1.0</MedicalTendSpeed>
            <MedicalTendQuality>1.0</MedicalTendQuality>
            <MedicalSurgerySuccessChance>1.0</MedicalSurgerySuccessChance>
            <RoyalFavorValue>6</RoyalFavorValue>
        </statBases>
        <comps>
            <li Class="MunoRaceLib.MunoComp.ThingCompProperties_Galactogen">
                <maxGalactogen>120</maxGalactogen>
                <minGalactogen>0</minGalactogen>
                <houseGalactogen>2</houseGalactogen>
                <GalactogenUIName>乳源质</GalactogenUIName>
                <GalactogenUIDes>用于显示乳源值的数量</GalactogenUIDes>
            </li>
        </comps>
        <race>
            <thinkTreeMain>Humanlike</thinkTreeMain>
            <thinkTreeConstant>HumanlikeConstant</thinkTreeConstant>
            <intelligence>Humanlike</intelligence>
            <makesFootprints>true</makesFootprints>
            <lifeExpectancy>300</lifeExpectancy>
            <leatherDef>Leather_Human</leatherDef>
            <nameCategory>HumanStandard</nameCategory>
            <hasGenders>True</hasGenders>
            <body>MunoGirlBody</body>
            <baseHungerRate>0.75</baseHungerRate>
            <baseBodySize>1</baseBodySize>
            <baseHealthScale>1.1</baseHealthScale>
            <foodType>OmnivoreHuman</foodType>
            <gestationPeriodDays>30</gestationPeriodDays>
            <renderTree>Humanlike</renderTree>
            <litterSizeCurve>
                <points>
                    <li>(0.5, 0)</li>
                    <li>(1, 1)</li>
                    <li>(1.01, 0.02)</li>
                    <li>(3.5, 0)</li>
                </points>
            </litterSizeCurve>
            <lifeStageAges>
                <li Class="AlienRace.LifeStageAgeAlien">
                    <def>HumanlikeBaby</def>
                    <minAge>0</minAge>
                    <customHeadDrawSize>(1.2,1.2)</customHeadDrawSize>
                </li>
                <li Class="AlienRace.LifeStageAgeAlien">
                    <def>HumanlikeChild</def>
                    <minAge>4</minAge>
                    <headOffset>(0.085,0.0)</headOffset>
                    <customDrawSize>(0.9,0.9)</customDrawSize>
                    <customHeadDrawSize>(1.2,1.2)</customHeadDrawSize>
                </li>
                <li Class="AlienRace.LifeStageAgeAlien" MayRequire="Ludeon.RimWorld.Biotech">
                    <def>HumanlikePreTeenager</def>
                    <minAge>9</minAge>
                    <headOffset>(0.054,-0.07)</headOffset>
                    <customDrawSize>(0.9,0.9)</customDrawSize>
                    <customHeadDrawSize>(1.2,1.2)</customHeadDrawSize>
                </li>
                <li Class="AlienRace.LifeStageAgeAlien">
                    <def>HumanlikeTeenager</def>
                    <minAge>13</minAge>
                    <headOffset>(-0.045,-0.03)</headOffset>
                </li>
                <li Class="AlienRace.LifeStageAgeAlien">
                    <def>HumanlikeAdult</def>
                    <minAge>16</minAge>
                    <headOffset>(-0.06,0.05)</headOffset>
                </li>
            </lifeStageAges>
            <ageGenerationCurve>
                <points>
                    <li MayRequire="Ludeon.RimWorld.Biotech">(0,0)</li>
                    <li MayRequire="Ludeon.RimWorld.Biotech">(0.001,43)</li>
                    <li MayRequire="Ludeon.RimWorld.Biotech">(8,86)</li>
                    <li MayRequire="Ludeon.RimWorld.Biotech">(12.5,118)</li>                    <!-- Exclude 12.5-13 years to avoid spawning pawns who are very close to becoming adults -->
                    <li MayRequire="Ludeon.RimWorld.Biotech">(12.51,0)</li>
                    <li MayRequire="Ludeon.RimWorld.Biotech">(13,0)</li>
                    <li MayRequire="Ludeon.RimWorld.Biotech">(13.001,122)</li>                    <!-- End exclude -->
                    <li MayRequire="Ludeon.RimWorld.Biotech">(13.999,130)</li>
                    <li>(14,0)</li>
                    <li MayRequire="Ludeon.RimWorld.Biotech">(14.001,20)</li>
                    <li>(16,60)</li>
                    <li>(20,100)</li>
                    <li>(30,30)</li>
                    <li>(60,5)</li>
                    <li>(80,1)</li>
                    <li>(100,0)</li>
                </points>
            </ageGenerationCurve>
            <hediffGiverSets>
                <li>Human</li>
                <li>OrganicStandard</li>
            </hediffGiverSets>
        </race>
    </AlienRace.ThingDef_AlienRace>
</Defs>