<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- 缪诺乳 -->
<ThingDef ParentName="OrganicProductBase">
    <defName>MunoRace_MunoMilk</defName>
    <label>缪诺乳</label>
    <description>从缪诺族身上获得的可食用液体，同时还有多种用途，但从哪里获得尚不可知</description>
    <graphicData>
      <texPath>AlienRace/MunoRace/Item/Resource/MunoRace_MunoMilk</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <socialPropernessMatters>true</socialPropernessMatters>
    <statBases>
      <MarketValue>2.2</MarketValue> <!-- Can be used like meat in recipes -->
      <Mass>0.045</Mass>
      <MaxHitPoints>60</MaxHitPoints>
      <Flammability>0.2</Flammability>
      <Nutrition>0.1</Nutrition>
      <FoodPoisonChanceFixedHuman>0.01</FoodPoisonChanceFixedHuman>
    </statBases>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>14</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
    <ingestible>
      <preferability>RawTasty</preferability>
      <foodType>AnimalProduct, Fluid</foodType>
      <babiesCanIngest>true</babiesCanIngest>
    </ingestible>
    <thingCategories>
      <li>AnimalProductRaw</li>
    </thingCategories>
    <allowedArchonexusCount>80</allowedArchonexusCount>
  </ThingDef>



</Defs>